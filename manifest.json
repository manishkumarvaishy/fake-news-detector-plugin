{
    "manifest_version": 2,
  
    "name": "Fake News Detection Plugin",
    "description": "This extension will detect fake news using your browser search tab.",
    "version": "1.0",
  
    "browser_action": {
     "default_icon": "icon.jpg",
     "default_popup": "popup.html"
    },
    
    "permissions": [
     "activeTab",
     "http://*/*",
    "https://*/*",
    "https://www.google.com/*",
    "http://www.google.com/*",
    "https://www.unslanted.net/*"
    
     ]

  }